{
  "name": "ai-interview-backend",
  "version": "1.0.0",
  "description": "Backend for AI Interview Bot with Gemini API, TTS/STT, Payment Gateway, Email, and Report Generation",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:js": "node server.js",
    "dev": "nodemon src/server.ts",
    "dev:no-check": "ts-node --transpile-only src/server.ts",
    "dev:js": "nodemon server.js",
    "build": "tsc",
    "build:no-check": "tsc --noEmitOnError false",
    "build:simple": "mkdir -p dist && cp -r src/* dist/ && find dist -name '*.ts' -exec sh -c 'mv \"$1\" \"${1%.ts}.js\"' _ {} \\;",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.41.0",
    "@google/generative-ai": "^0.2.1",
    "openai": "^4.24.1",
    "@google-cloud/text-to-speech": "^5.1.0",
    "@google-cloud/speech": "^6.0.1",

    "razorpay": "^2.9.2",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0",
    "puppeteer": "^21.6.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "socket.io": "^4.7.4",
    "redis": "^4.6.11",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "cron": "^3.1.6",
    "sharp": "^0.33.1",
    "xlsx": "^0.18.5",
    "csv-parse": "^5.5.3",
    "axios": "^1.6.2",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfkit": "^0.12.12",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2"
  },
  "keywords": [
    "ai",
    "interview",
    "bot",
    "gemini",
    "tts",
    "stt",
    "payment",
    "backend"
  ],
  "author": "AI Interview Bot",
  "license": "MIT"
}